shell /usr/bin/bash

kitty_mod ctrl+shift

# BEGIN_KITTY_THEME
# Catppuccin-Mocha
include current-theme.conf
# END_KITTY_THEME

font_family Hack Nerd Font
bold_font Hack Nert Font Bold
italic_font Hack Nerd Font Italic
bold_italic_font Hack Nerd Font Bold Italic
font_size 14


foreground #cdd6f4
background #1e1e2e

# Catppuccin Colors for Prompt
color0  #45475a
color1  #f38ba8
color2  #a6e3a1
color3  #f9e2af
